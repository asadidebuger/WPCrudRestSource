                                <subModelOperations xsi:type="tool_1:SetValue" featureName="field" valueExpression="service:getFirstItemFromList(selection)"/>
                              </subModelOperations>
                            </subModelOperations>
                          </firstModelOperations>
                        </onClickOperation>
                      </controls>
                      <controls xsi:type="properties:ListDescription" name="fieldLayouts" labelExpression="Form Fields" helpExpression="This fields will be accessible in form view" valueExpression="aql:self.fieldLayout" displayExpression="service:getFieldLayoutLabel(value)">
                        <onClickOperation>
                          <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:element">
                            <subModelOperations xsi:type="tool_1:If" conditionExpression="service:canRemoveFieldForm(onClickEventKind,selection)">
                              <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self">
                                <subModelOperations xsi:type="tool_1:Unset" featureName="fieldLayout" elementExpression="aql:selection"/>
                              </subModelOperations>
                            </subModelOperations>
                          </firstModelOperations>
                        </onClickOperation>
                      </controls>
                      <layout xsi:type="properties:GridLayoutDescription" numberOfColumns="2" makeColumnsWithEqualWidth="true"/>
                    </controls>
                    <controls xsi:type="properties:ContainerDescription">
                      <controls xsi:type="properties:RadioDescription" name="rdbLayout" labelExpression="Layout" helpExpression="Select a layout for selected field" isEnabledExpression="service:notNullselectedFormField()" valueExpression="service:getSelectedFormFieldLayout()" candidatesExpression="aql:self.ancestors(crudrest::PluginGUI).reusableLayout" candidateDisplayExpression="service:getLayoutLabel(candidate)" numberOfColumns="1">
                        <initialOperation>
                          <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="service:getSelectedFormField()">
                            <subModelOperations xsi:type="tool_1:SetValue" featureName="layout" valueExpression="var:newValue"/>
                          </firstModelOperations>
                        </initialOperation>
                      </controls>
                      <layout xsi:type="properties:GridLayoutDescription"/>
                    </controls>
                    <style barStyle="NO_TITLE" toggleStyle="NONE" expandedByDefault="true"/>
                  </groups>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
